<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/awalFragment">

    <fragment
        android:id="@+id/awalFragment"
        android:name="com.example.sakatap.ui.mulai.AwalFragment"
        android:label="AwalFragment"
        tools:layout="@layout/fragment_awal"
        app:startDestination="@id/action_awalFragment_to_signupFragment">
        <action
            android:id="@+id/action_awalFragment_to_loginFragment"
            app:destination="@id/loginFragment"
            app:enterAnim="@anim/fragment_fade_enter"
            app:exitAnim="@anim/fragment_fade_exit" />
        <action
            android:id="@+id/action_awalFragment_to_signupFragment"
            app:destination="@id/signupFragment"
            app:enterAnim="@anim/fragment_fade_enter"
            app:exitAnim="@anim/fragment_fade_exit" />
    </fragment>
    <fragment
        android:id="@+id/overviewFragment"
        android:name="com.example.sakatap.ui.overview.OverviewFragment"
        android:label="fragment_umpak"
        tools:layout="@layout/fragment_umpak" >
        <action
            android:id="@+id/action_overviewFragment_to_gapuraPertamaFragment"
            app:destination="@id/gapuraPertamaFragment" />
        <action
            android:id="@+id/action_overviewFragment_to_guideFragment"
            app:destination="@id/guideFragment" />
    </fragment>
    <fragment
        android:id="@+id/candiMiniaturFragment"
        android:name="com.example.sakatap.ui.tempat.CandiMiniaturFragment"
        android:label="CandiMiniaturFragment"
        tools:layout="@layout/fragment_candi_miniatur">
        <action
            android:id="@+id/action_candiMiniaturFragment_to_kolamKeputrenFragment"
            app:destination="@id/kolamKeputrenFragment" />
        <action
            android:id="@+id/action_candiMiniaturFragment_to_guideFragment"
            app:destination="@id/guideFragment" />
    </fragment>
    <fragment
        android:id="@+id/pasebanFragment"
        android:name="com.example.sakatap.ui.tempat.PasebanFragment"
        android:label="PasebanFragment"
        tools:layout="@layout/fragment_paseban">
        <action
            android:id="@+id/action_pasebanFragment_to_pendapaFragment"
            app:destination="@id/pendapaFragment" />
        <action
            android:id="@+id/action_pasebanFragment_to_guideFragment"
            app:destination="@id/guideFragment" />
    </fragment>
    <fragment
        android:id="@+id/candiPembakaranFragment"
        android:name="com.example.sakatap.ui.tempat.CandiPembakaranFragment"
        android:label="CandiPembakaranFragment"
        tools:layout="@layout/fragment_candi_pembakaran">
        <action
            android:id="@+id/action_candiPembakaranFragment_to_umpakFragment"
            app:destination="@id/umpakFragment" />
        <action
            android:id="@+id/action_candiPembakaranFragment_to_guideFragment"
            app:destination="@id/guideFragment" />
    </fragment>
    <fragment
        android:id="@+id/loginFragment"
        android:name="com.example.sakatap.ui.login.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_loginFragment_to_lupapasswordFragment"
            app:destination="@id/lupapasswordFragment" />
        <action
            android:id="@+id/action_loginFragment_to_signupFragment"
            app:destination="@id/signupFragment" />
        <action
            android:id="@+id/action_loginFragment_to_welcomeFragment"
            app:destination="@id/welcomeFragment" />
    </fragment>
    <fragment
        android:id="@+id/signupFragment"
        android:name="com.example.sakatap.ui.signup.SignupFragment"
        android:label="fragment_signup"
        tools:layout="@layout/fragment_signup" >
        <action
            android:id="@+id/action_signupFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
        <action
            android:id="@+id/action_signupFragment_to_welcomeFragment"
            app:destination="@id/welcomeFragment" />
    </fragment>
    <fragment
        android:id="@+id/lupapasswordFragment"
        android:name="com.example.sakatap.ui.login.lupapasswordFragment"
        android:label="Lupa Password"
        tools:layout="@layout/fragment_lupa_password">
        <action
            android:id="@+id/action_lupapasswordFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
    </fragment>
    <fragment
        android:id="@+id/welcomeFragment"
        android:name="com.example.sakatap.ui.welcome.WelcomeFragment"
        android:label="fragment_welcome"
        tools:layout="@layout/fragment_welcome" >
        <action
            android:id="@+id/action_welcomeFragment_to_nfcAwalFragment"
            app:destination="@id/nfcAwalFragment" />
        <action
            android:id="@+id/action_welcomeFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
    </fragment>
    <fragment
        android:id="@+id/nfcAwalFragment"
        android:name="com.example.sakatap.ui.nfc.NfcAwalFragment"
        android:label="fragment_nfc_awal"
        tools:layout="@layout/fragment_nfc_awal" >
        <action
            android:id="@+id/action_nfcAwalFragment_to_overviewFragment"
            app:destination="@id/overviewFragment" />
    </fragment>
    <fragment
        android:id="@+id/gapuraPertamaFragment"
        android:name="com.example.sakatap.ui.tempat.GapuraPertamaFragment"
        android:label="fragment_gapura_utama"
        tools:layout="@layout/fragment_gapura_utama" >
        <action
            android:id="@+id/action_gapuraPertamaFragment_to_sejarahPenghunianFragment"
            app:destination="@id/sejarahPenghunianFragment" />
        <action
            android:id="@+id/action_gapuraPertamaFragment_to_guideFragment"
            app:destination="@id/guideFragment" />
    </fragment>
    <fragment
        android:id="@+id/sejarahPenghunianFragment"
        android:name="com.example.sakatap.ui.tempat.SejarahPenghunianFragment"
        android:label="fragment_sejarah_penghunian"
        tools:layout="@layout/fragment_sejarah_penghunian" >
        <action
            android:id="@+id/action_sejarahPenghunianFragment_to_gapuraKeduaFragment"
            app:destination="@id/gapuraKeduaFragment" />
        <action
            android:id="@+id/action_sejarahPenghunianFragment_to_guideFragment"
            app:destination="@id/guideFragment" />
    </fragment>
    <fragment
        android:id="@+id/gapuraKeduaFragment"
        android:name="com.example.sakatap.ui.tempat.GapuraKeduaFragment"
        android:label="fragment_gapura_kedua"
        tools:layout="@layout/fragment_gapura_kedua" >
        <action
            android:id="@+id/action_gapuraKeduaFragment_to_taludFragment"
            app:destination="@id/taludFragment" />
        <action
            android:id="@+id/action_gapuraKeduaFragment_to_guideFragment"
            app:destination="@id/guideFragment" />
    </fragment>
    <fragment
        android:id="@+id/taludFragment"
        android:name="com.example.sakatap.ui.tempat.TaludFragment"
        android:label="fragment_talud"
        tools:layout="@layout/fragment_talud" >
        <action
            android:id="@+id/action_taludFragment_to_candiPembakaranFragment"
            app:destination="@id/candiPembakaranFragment" />
        <action
            android:id="@+id/action_taludFragment_to_guideFragment"
            app:destination="@id/guideFragment" />
    </fragment>
    <fragment
        android:id="@+id/umpakFragment"
        android:name="com.example.sakatap.ui.tempat.UmpakFragment"
        android:label="fragment_umpak"
        tools:layout="@layout/fragment_umpak" >
        <action
            android:id="@+id/action_umpakFragment_to_pasebanFragment"
            app:destination="@id/pasebanFragment" />
        <action
            android:id="@+id/action_umpakFragment_to_guideFragment"
            app:destination="@id/guideFragment" />
    </fragment>
    <fragment
        android:id="@+id/pendapaFragment"
        android:name="com.example.sakatap.ui.tempat.PendapaFragment"
        android:label="fragment_pendapa"
        tools:layout="@layout/fragment_pendapa" >
        <action
            android:id="@+id/action_pendapaFragment_to_candiMiniaturFragment"
            app:destination="@id/candiMiniaturFragment" />
        <action
            android:id="@+id/action_pendapaFragment_to_guideFragment"
            app:destination="@id/guideFragment" />
    </fragment>
    <fragment
        android:id="@+id/kolamKeputrenFragment"
        android:name="com.example.sakatap.ui.tempat.KolamKeputrenFragment"
        android:label="fragment_keputren"
        tools:layout="@layout/fragment_keputren" >
        <action
            android:id="@+id/action_kolamKeputrenFragment_to_guideFragment"
            app:destination="@id/guideFragment" />
        <action
            android:id="@+id/action_kolamKeputrenFragment_to_guaFragment"
            app:destination="@id/guaFragment" />
    </fragment>
    <fragment
        android:id="@+id/guaFragment"
        android:name="com.example.sakatap.ui.tempat.GuaFragment"
        android:label="fragment_gua"
        tools:layout="@layout/fragment_gua" >
        <action
            android:id="@+id/action_guaFragment_to_guideFragment"
            app:destination="@id/guideFragment" />
    </fragment>
    <fragment
        android:id="@+id/guideFragment"
        android:name="com.example.sakatap.ui.guide.GuideFragment"
        android:label="fragment_guide"
        tools:layout="@layout/fragment_guide" >
        <action
            android:id="@+id/action_guideFragment_to_nfcAkhirFragment"
            app:destination="@id/nfcAkhirFragment" />
        <action
            android:id="@+id/action_guideFragment_to_gapuraPertamaFragment"
            app:destination="@id/gapuraPertamaFragment" />
        <action
            android:id="@+id/action_guideFragment_to_sejarahPenghunianFragment"
            app:destination="@id/sejarahPenghunianFragment" />
        <action
            android:id="@+id/action_guideFragment_to_gapuraKeduaFragment"
            app:destination="@id/gapuraKeduaFragment" />
        <action
            android:id="@+id/action_guideFragment_to_taludFragment"
            app:destination="@id/taludFragment" />
        <action
            android:id="@+id/action_guideFragment_to_candiPembakaranFragment"
            app:destination="@id/candiPembakaranFragment" />
        <action
            android:id="@+id/action_guideFragment_to_umpakFragment"
            app:destination="@id/umpakFragment" />
        <action
            android:id="@+id/action_guideFragment_to_pasebanFragment"
            app:destination="@id/pasebanFragment" />
        <action
            android:id="@+id/action_guideFragment_to_pendapaFragment"
            app:destination="@id/pendapaFragment" />
        <action
            android:id="@+id/action_guideFragment_to_candiMiniaturFragment"
            app:destination="@id/candiMiniaturFragment" />
        <action
            android:id="@+id/action_guideFragment_to_kolamKeputrenFragment"
            app:destination="@id/kolamKeputrenFragment" />
        <action
            android:id="@+id/action_guideFragment_to_guaFragment"
            app:destination="@id/guaFragment" />
    </fragment>
    <fragment
        android:id="@+id/mapFragment"
        android:name="com.example.sakatap.ui.map.MapFragment"
        android:label="Peta"
        tools:layout="@layout/fragment_map" />
    <fragment
        android:id="@+id/nfcAkhirFragment"
        android:name="com.example.sakatap.ui.nfc.NfcAkhirFragment"
        android:label="fragment_nfc_akhir"
        tools:layout="@layout/fragment_nfc_akhir" >
        <action
            android:id="@+id/action_nfcAkhirFragment_to_poinFragment"
            app:destination="@id/poinFragment" />
    </fragment>
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.example.sakatap.ui.Profile.ProfileFragment"
        android:label="Profil"
        tools:layout="@layout/fragment_profile">
        <action
            android:id="@+id/action_profileFragment_to_ubahPasswordFragment"
            app:destination="@id/ubahPasswordFragment" />
        <action
            android:id="@+id/action_profileFragment_to_awalFragment"
            app:destination="@id/awalFragment" />
    </fragment>
    <fragment
        android:id="@+id/ubahPasswordFragment"
        android:name="com.example.sakatap.ui.Profile.UbahPasswordFragment"
        android:label="Ubah Password"
        tools:layout="@layout/fragment_ubah_password" />
    <fragment
        android:id="@+id/souvenirFragment"
        android:name="com.example.sakatap.ui.souvenir.SouvenirFragment"
        android:label="Souvenir"
        tools:layout="@layout/fragment_souvenir">
        <action
            android:id="@+id/action_souvenirFragment_to_showFragment"
            app:destination="@id/showFragment" />
    </fragment>
    <fragment
        android:id="@+id/showFragment"
        android:name="com.example.sakatap.ui.souvenir.ShowFragment"
        android:label="fragment_show"
        tools:layout="@layout/fragment_show" />
    <fragment
        android:id="@+id/poinFragment"
        android:name="com.example.sakatap.ui.poin.PoinFragment"
        android:label="fragment_poin"
        tools:layout="@layout/fragment_poin" >
        <action
            android:id="@+id/action_poinFragment_to_souvenirFragment"
            app:destination="@id/souvenirFragment" />
        <action
            android:id="@+id/action_poinFragment_to_welcomeFragment"
            app:destination="@id/welcomeFragment" />
    </fragment>
</navigation>